<h1>Tutorials</h1>

<h2>Search <%= text_field_tag 'search', '', data: {'js-search' => true} %></h2>
<ul>
    <% @tutorials.each do |tutorial| %>
        <li><%= tutorial.title %> <%= link_to "Fade", "", class: 'fade', data: {'js-fade-link' => true} %></li>
        <li><%= tutorial.url %> <%= link_to "Hide", "", class: 'hide', data: {'js-hide-link' => true} %></li>
    <% end %>
</ul>